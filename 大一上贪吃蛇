# -Curriculum-Design
记录我的学习
#include<stdio.h>
#include<graphics.h>
#include<time.h>
#include<conio.h>
# include <string.h>
#include<windows.h>
typedef
struct 
{
	char name[100];
	int score;
}Player;
typedef
struct 
{
	int x,y;
}Food;
Food food;
Player people[6],t;
int score=0;
char name[100];
void phb();
int kaishiyouxi();
int shezhi();
int paihangbang();
int guizhe();
int x[200]={300,310,320},y[200]={100,100,100},aa,nn,hh7,sum1,hh6,xiao3,hh3,m,xiao1,xiao4,ch8,xiao2,hh1,hh2,i,d=3,n=3,hh,kk,ch6,key,stop,h=4,ch,q,ch1,ayaco,k,ch7,xiao;
int flag=0;
char str[100],ch2[100],ch4[100],ch3[100],ch5[100];
FILE *fp;
main ()
{
   initwindow(640,480);
   while(1)
   {
    
   	memset(ch4,'\0',sizeof(ch4));
	memset(ch2,'\0',sizeof(ch2));
	setcolor(WHITE);
	rectangle(48,48,512,412);
	outtextxy(30,30,"greedysnake");
	circle(450,375,30);
	outtextxy(425,370,"E:设置"); 
	rectangle(220,130,340,180);
	rectangle(220,210,340,260);
	rectangle(220,290,340,340);
	outtextxy(235,150,"A:开始游戏");
	outtextxy(235,230,"B:继续游戏");
	outtextxy(235,310,"C:游戏规则");
	ch1=getch();
	switch(ch1)
			{
				case 'A': ayaco=1;break;
				case 'B': ayaco=3;break;
				case 'C': ayaco=4;break;
				case '\0':getch();	
			}
	setcolor(BLACK);
	rectangle(220,130,340,180);
	rectangle(220,210,340,260);
	rectangle(220,290,340,340);
	outtextxy(235,150,"A:开始游戏");
	outtextxy(235,230,"B:继续游戏");
	outtextxy(235,310,"C:游戏规则");
	circle(450,375,30);
	outtextxy(425,370,"E:设置"); 
   if(ayaco==1)
	{ 
	   hh2=kaishiyouxi();
	   if(hh2==1)
	   {
	   	setcolor(BLACK);
	outtextxy(300,25,"排行榜"); 
	   	for(i=0;i<=4;i++)
	    {
		rectangle(200,50+i*50,320,100+i*50);
		rectangle(320,50+i*50,440,100+i*50);
      	}
	        for(i=0;i<=4;i++)
	         { 
			 outtextxy(220,75+i*50,people[i].name);
	         itoa(people[i].score,str,10);
	         outtextxy(340,75+i*50,str);
            }
           rectangle(270,330,370,360);
            rectangle(270,370,370,400);
	        outtextxy(280,335,"F:返回主界面");
	        outtextxy(300,375,"T:退出");     
	   }
	   else
	   if(hh2==3)
	   {
	   	closegraph();
	   	initwindow(640,480); 
	   }
	   else
	   break;
	}
	else
	if(ayaco==3)
	{
	 hh3=shezhi();
	 if(hh3==1)
	 {
	 	setcolor(BLACK);
	 	outtextxy(300,25,"排行榜"); 
	   	for(i=0;i<=4;i++)
	    {
		rectangle(200,50+i*50,320,100+i*50);
		rectangle(320,50+i*50,440,100+i*50);
      	}
	        for(i=0;i<=4;i++)
	         { 
			 outtextxy(220,75+i*50,people[i].name);
	         itoa(people[i].score,str,10);
	         outtextxy(340,75+i*50,str);
            }
             rectangle(270,330,370,360);
            rectangle(270,370,370,400);
	        outtextxy(280,335,"F:返回主界面");
	        outtextxy(300,375,"T:退出");   
	 }
	 else
	 if(hh3==3)
	 break;
	 else
	 {
	 	setcolor(BLACK);
	 	outtextxy(200,220,"用户名或密码错误");//出现不了字。
	    rectangle(320,330,370,350);
	    outtextxy(325,333,"F:返回");
	 }
	}
	else
	if(ayaco==4)	
	{
		hh=guizhe();
		if(hh==2)
		break;
		else
		{
		setcolor(BLACK);
	outtextxy(200,150,"w代表向上转弯，");
	outtextxy(200,175,"s代表向下转弯，");
	outtextxy(200,200,"a代表向左转弯，");
	outtextxy(200,225,"d代表向右转弯，");
	outtextxy(145,250,"通过这四个键进行贪吃蛇的移动，"); 
	outtextxy(145,275,"每吃一个食物，分数就增加100分，"); 
	outtextxy(145,300,"一旦撞到墙壁或撞到自身就会死亡。"); 
	outtextxy(145,325,"一旦撞到墙壁或撞到自身就会死亡。");//这句不显示。 
	circle(100,375,30);
	outtextxy(75,370,"F:返回"); 
	circle(450,375,30);
    outtextxy(425,370,"T:退出");
	outtextxy(450,370,"T:退出");//这句不显示。 
		} 	
	}
   }
   closegraph();	
}
void phb()
{	 
   int i,j;
	char str[20];
	FILE *fp;
	strcpy(people[5].name,name);
	people[5].score=score;
	if((fp=fopen("排行榜","rb"))!=NULL)
	{ 
	  fread(people,5*sizeof(Player),1,fp);
	  fclose(fp);
    }
	else
	{    
		   for(i=0;i<5;i++)
		   { strcpy(people[i].name,"nobody");
			 people[i].score=0;		 
		   }
			fp=fopen("排行榜","wb");
		    fwrite(people,5*sizeof(Player),1,fp);
		    fclose(fp);
	}
	for(i=1;i<=5;i++)
	{
		for(j=0;j<=5-i;j++)
		{  
		  if(people[j].score<people[j+1].score)
			{ 
			t=people[j];
			people[j]=people[j+1];
			people[j+1]=t;
			}
		}
	}		
	setcolor(YELLOW);
	outtextxy(300,25,"排行榜"); 
	   	setcolor(WHITE);
	   	for(i=0;i<=4;i++)
	    {
		rectangle(200,50+i*50,320,100+i*50);
		rectangle(320,50+i*50,440,100+i*50);
      	}
	        for(i=0;i<=4;i++)
	         { 
	         if(i==0)
	         setcolor(RED);
	         else
	         if(i==1)
	         setcolor(YELLOW);
	         else
	         if(i==2)
	         setcolor(GREEN);
	         else
	         if(i==3)
	         setcolor(BLUE);
	         else
			 setcolor(WHITE);
			 outtextxy(220,75+i*50,people[i].name);
	         itoa(people[i].score,str,10);
	         outtextxy(340,75+i*50,str);
            }
	fp=fopen("排行榜","wb");
    fwrite(people,5*sizeof(Player),1,fp);
	fclose(fp);
}
int kaishiyouxi()
{
	setcolor(WHITE);
	outtextxy(220,180,"用户名：");
	rectangle(220,190,340,210);
	outtextxy(220,220,"密码：(至少需要2位)");
	rectangle(220,230,340,250);
	ch2[0]=getch();
	k=1;
	while(1)
	{
		outtextxy(220+10*k,195,&ch2[k-1]);
		ch2[k]=getch();
		k++;
		if(ch2[k-1]==13)
		{
			ch2[k-1]='\0';
			break;
		}
		
	}
	strcpy(people[5].name,ch2);
    ch4[0]=getch();
	k=1;
	outtextxy(220+10*k,235,&ch4[0]);
	ch4[k]=getch();
	k++;
	while(1)
	{
	  	setcolor(BLACK);
	  	outtextxy(220+10*(k-1),235,&ch4[k-2]);
	  	setcolor(WHITE);
		outtextxy(220+10*k,235,&ch4[k-1]);
	  	outtextxy(220+10*(k-1),235,"*");
		ch4[k]=getch();
		k++;
		if(ch4[k-1]==13)
		{
			outtextxy(220+10*(k-1),235,"*");
			ch4[k-1]='\0';
			break; 	
		}
	} 
	setcolor(BLACK);
	outtextxy(220,180,"用户名：");
	rectangle(220,190,340,210);
	outtextxy(220,220,"密码：(至少需要2位)");
	rectangle(220,230,340,250);
	outtextxy(220+10,195,&ch2[0]);
	outtextxy(220+10,235,&ch4[0]);
	strcpy(name,ch2);
	while(1)
	{
	n=3;
	d=3;
    score=0;
	memset(x,0,sizeof(x));
	memset(y,0,sizeof(y));
	y[0]=100;
	y[1]=100;
	y[2]=100;
	x[0]=300;
	x[1]=310;
	x[2]=320;
	kk=0;
	xiao2=0;
	m=4;
	setcolor(WHITE);
	rectangle(48,48,512,412);
	outtextxy(30,30,"greedysnake");
	rectangle(250,130,320,180);
	rectangle(250,210,320,260);
	rectangle(250,290,320,340);
	outtextxy(260,150,"a:简单");
	outtextxy(260,230,"b:中等");
	outtextxy(260,310,"c:困难");
	ch=getch();
	switch(ch)
			{
				case 'a':q=500;break;
				case 'b':q=300;break;
				case 'c':q=100;break;
			}
	setcolor(BLACK);
	rectangle(250,130,320,180);
	rectangle(250,210,320,260);
	rectangle(250,290,320,340);
	outtextxy(260,150,"a:简单");
	outtextxy(260,230,"b:中等");
	outtextxy(260,310,"c:困难");
	setcolor(RED);
	outtextxy(240,8,"生命值"); 
	setcolor(YELLOW);
	circle(250,30,5);
	circle(265,30,5);
	circle(280,30,5);
	setcolor(WHITE);
    outtextxy(400,30,"score:");
	flag=0;
	itoa(score,str,10);	
	outtextxy(450,30,str);
	setcolor(3);
	for(i=0;i<n;i++)
	{
		rectangle(x[i],y[i],x[i]+10,y[i]+10);
		setcolor(h);
		h++;
	}	
	h=4;
	while(1)
	{
        nn=rand()%10+6;
        setcolor(nn);
        rectangle(48,48,512,412);
		if(flag==0)
		{
			srand((unsigned)time(NULL));
			food.x=rand()%400+50;
			food.y=rand()%300+50;
			while(food.x%10!=0)food.x++;
			while(food.y%10!=0)food.y++;
			flag=1;
			setcolor(YELLOW);
			rectangle(food.x,food.y,food.x+10,food.y+10);
		}
		if(food.x==x[0]&&food.y==y[0])
		{
			setcolor(BLACK);
			rectangle(food.x,food.y,food.x+10,food.y+10);
			n++;
			flag=0;
			score+=100;
			itoa(score,str,10);
			setcolor(m);
			outtextxy(450,30,str);
			m++; 
		}
		Sleep(q);
		setcolor(BLACK);
		for(i=0;i<n;i++)
		{
				rectangle(x[i],y[i],x[i]+10,y[i]+10);
		}
		setcolor(YELLOW);
		rectangle(food.x,food.y,food.x+10,food.y+10);
		setcolor(5);
		rectangle(48,48,512,412);
		for(i=n-1;i>=1;i--)
		{
			x[i]=x[i-1];
			y[i]=y[i-1];
		}
		aa=d;
		if(kbhit()!=0)
		{
			key=getch();
			switch(key)
			{
				case 119:if(d!=2)d=1;break;
				case 115:if(d!=1)d=2;break;
				case 97:if(d!=4)d=3;break;
				case 100:if(d!=3)d=4;break;
				case  27:d=5;break;
			}
		}
		if(d==5)
		{
		 fp=fopen("score.txt","wb");
		    fwrite(&score,sizeof(score),1,fp);
		    fclose(fp);
		fp=fopen("n.txt","wb");
		    fwrite(&n,sizeof(n),1,fp);
		    fclose(fp);
		fp=fopen("food.txt","wb");
		    fwrite(&food,sizeof(Food),1,fp);
		    fclose(fp);
        fp=fopen("x.txt","wb");
		    fwrite(x,n*sizeof(int),1,fp);
		    fclose(fp);
		fp=fopen("y.txt","wb");
		    fwrite(y,n*sizeof(int),1,fp);
		    fclose(fp);
		fp=fopen("h.txt","wb");
		    fwrite(&h,sizeof(h),1,fp);
		    fclose(fp);
		fp=fopen("q.txt","wb");
		    fwrite(&q,sizeof(q),1,fp);
		    fclose(fp);
		fp=fopen("用户名.txt","wb");
		    fwrite(ch2,sizeof(char [100]),1,fp);
		    fclose(fp);
		fp=fopen("密码.txt","wb");
		    fwrite(ch4,sizeof(char [100]),1,fp);
		    fclose(fp);
		fp=fopen("kk.txt","wb");
		    fwrite(&kk,sizeof(kk),1,fp);
		    fclose(fp);
		fp=fopen("d.txt","wb");
		    fwrite(&aa,sizeof(aa),1,fp);
		    fclose(fp);
		fp=fopen("m.txt","wb");
		    fwrite(&m,sizeof(m),1,fp);
		    fclose(fp);
		fp=fopen("nn.txt","wb");
		    fwrite(&nn,sizeof(nn),1,fp);
		    fclose(fp);
		    sum1=1;
		break;	
		}
		else
		{
				switch(d)
		{	
			case 1:y[0]-=10;break;
			case 2:y[0]+=10;break;
			case 3:x[0]-=10;break;
			case 4:x[0]+=10;break;
		}
		for(i=3;i<n;i++)
		{
			if(x[i]==x[0]&&y[i]==y[0])
				stop=1;
		}
			if(stop==1)
				break;
		setcolor(3);
		for(i=0;i<n;i++)
		{
			rectangle(x[i],y[i],x[i]+10,y[i]+10);
			setcolor(h);
			h++;
		}
		
		}
			h=4;
		if(x[0]<50&&x[0]>0||y[0]<50&&y[0]>0||x[0]>500&&x[0]<640||y[0]>400&&y[0]<480)
			{	
				kk+=1;
			}
			if(kk==3)
			{
			 	setcolor(BLACK);
				outtextxy(400,15,"还剩下一条命");
				setcolor(RED);
				outtextxy(400,15,"已无命留下");
				setcolor(BLACK); 
	            circle(280,30,5);
				break;
			}
			else
			if(kk==2)
			{
				setcolor(BLACK);
			    outtextxy(400,15,"还剩下两条命");
				setcolor(RED);
				outtextxy(400,15,"还剩下一条命");
				setcolor(BLACK);
				circle(250,30,5);
			}	
			else
			if(kk==1)
			{
				setcolor(BLACK);
				outtextxy(400,15,"还有三条命");
				setcolor(RED);
			    outtextxy(400,15,"还剩下两条命");
				setcolor(BLACK);
	            circle(265,30,5);	
			}
			else
			if(kk==0) 
			{
				setcolor(RED);
				outtextxy(400,15,"还有三条命");
			}
	}
		if(d!=5)
	  {
	    setcolor(YELLOW);
	    outtextxy(250,240,"GAME OVER!"); 
	closegraph();
	initwindow(640,480);
	setcolor(WHITE);
    rectangle(48,48,512,412);
	outtextxy(200,200,"您的得分为：");
	itoa(score,str,10);
	outtextxy(285,200,str);
	rectangle(305,330,370,350);
	rectangle(380,330,430,350);
	outtextxy(310,333,"F:排行榜");
	outtextxy(385,333,"C:重来");
	outtextxy(385,333,"C:重来");
	ch8=getch();
	switch(ch8)
			{
				case 'F': xiao2=1;break;
				case 'C': xiao2=2;break;
				
			}
			if(xiao2==1)
			{
				setcolor(BLACK);
    rectangle(48,48,512,412);
	outtextxy(200,200,"您的得分为：");
	itoa(score,str,10);
	outtextxy(285,200,str);
	rectangle(320,330,370,350);
	rectangle(380,330,430,350);
	outtextxy(315,333,"F:排行榜");
	outtextxy(385,333,"C:重来");
				phb();
		        hh6=paihangbang();
				 if(hh6==1)
				 xiao4=1;
				 else
				 if(hh6==2)
				 xiao4=2; 
				 break;
			}
			else
			if(xiao2==2)
			{ 
			setcolor(BLACK);
    rectangle(48,48,512,412);
	outtextxy(200,200,"您的得分为：");
	itoa(score,str,10);
	outtextxy(285,200,str);
	rectangle(305,330,370,350);
	rectangle(380,330,430,350);
	outtextxy(310,333,"F:排行榜");
	outtextxy(385,333,"C:重来");	
			}
		}	
		if(sum1==1) 
		{
			xiao4=3;
			break;
		}
	}  
	return xiao4;
}
int paihangbang()
{

            rectangle(270,330,370,360);
            rectangle(270,370,370,400);
            setcolor(WHITE);
	        outtextxy(280,335,"F:返回主界面");
	        outtextxy(300,375,"T:退出");
			ch6=getch();
	switch(ch6)
			{
				case 'F': xiao1=1;break;
				case 'T': xiao1=2;break;
				case '\0':getch();	
			}
			return xiao1;           
}
int shezhi()
{
		setcolor(WHITE);
	outtextxy(220,180,"用户名：");
	rectangle(220,190,340,210);
	outtextxy(220,220,"密码：(至少需要2位)");
	rectangle(220,230,340,250);
	fp=fopen("用户名.txt","rb");
		    fread(ch2,sizeof(char [100]),1,fp);
		    fclose(fp);
	fp=fopen("密码.txt","rb");
		    fread(ch4,sizeof(char [100]),1,fp);
		    fclose(fp);
		fp=fopen("score.txt","rb");
		    fread(&score,sizeof(score),1,fp);  
		    fclose(fp);
		fp=fopen("n.txt","rb");
		    fread(&n,sizeof(n),1,fp);  
		    fclose(fp);
		fp=fopen("food.txt","rb");
		    fread(&food,sizeof(Food),1,fp);  
		    fclose(fp);
        fp=fopen("x.txt","rb");
		    fread(x,n*sizeof(int),1,fp);  
		    fclose(fp);
		fp=fopen("y.txt","rb");
		    fread(y,n*sizeof(int),1,fp);
		    fclose(fp);
		fp=fopen("h.txt","rb");
		    fread(&h,sizeof(h),1,fp);
		    fclose(fp);
		fp=fopen("q.txt","rb");
		    fread(&q,sizeof(q),1,fp);
		    fclose(fp);
		fp=fopen("kk.txt","rb");
		    fread(&kk,sizeof(kk),1,fp);
		    fclose(fp);
		fp=fopen("d.txt","rb");
		    fread(&d,sizeof(d),1,fp);
		    fclose(fp);
		fp=fopen("m.txt","rb");
		    fread(&m,sizeof(m),1,fp);
		    fclose(fp);
		fp=fopen("nn.txt","rb");
		    fread(&nn,sizeof(nn),1,fp);
		    fclose(fp);
		    ch3[0]=getch();
	k=1;
	while(1)
	{
		outtextxy(220+10*k,195,&ch3[k-1]);
		ch3[k]=getch();
		k++;
		if(ch3[k-1]==13)
		{
			ch3[k-1]='\0';
				break;
		}
	}
    ch5[0]=getch();
	k=1;
	outtextxy(220+10*k,235,&ch5[0]);
	ch5[k]=getch();
	k++;
	while(1)
	{
	  	setcolor(BLACK);
	  	outtextxy(220+10*(k-1),235,&ch5[k-2]);
	  	setcolor(WHITE);
		outtextxy(220+10*k,235,&ch5[k-1]);
	  	outtextxy(220+10*(k-1),235,"*");
		ch5[k]=getch();
		k++;
		if(ch5[k-1]==13)
		{
			outtextxy(220+10*(k-1),235,"*");
			ch5[k-1]='\0';	
					break; 	
		}
	} 
	setcolor(BLACK);
	outtextxy(220,180,"用户名：");
	rectangle(220,190,340,210);
	outtextxy(220,220,"密码：(至少需要2位)");
	rectangle(220,230,340,250);
	outtextxy(220+10,195,&ch3[0]);
	outtextxy(220+10,235,&ch5[0]);
    if(strcmp(ch3,ch2)==0&&strcmp(ch5,ch4)==0)
    {
        setcolor(nn);
        rectangle(48,48,512,412);
    			    setcolor(YELLOW);
			rectangle(food.x,food.y,food.x+10,food.y+10);
			flag=1;
		    setcolor(WHITE);
    outtextxy(400,30,"score:");
	itoa(score,str,10);	
	setcolor(m);
	outtextxy(450,30,str);
	m++;
	setcolor(RED);
	outtextxy(240,8,"生命值");
	if(kk==0)
	{
	setcolor(YELLOW);
	circle(250,30,5);
	circle(265,30,5);
	circle(280,30,5);
	setcolor(RED);
	outtextxy(400,15,"还有三条命");
	}
	else
	if(kk==1)
	{
	setcolor(YELLOW);
	circle(265,30,5);
	circle(280,30,5);	
	setcolor(RED);
	outtextxy(400,15,"还剩下两条命");		
	}
	else
	if(kk==2)
	{
	setcolor(YELLOW);
	circle(280,30,5);	
	setcolor(RED);
	outtextxy(400,15,"还剩下一条命");
	}	
	h=h-n+3-1;
	//刚开始少一个蛇身，且颜色不对，走几步后又对啦 。 
	setcolor(h);
	for(i=0;i<n;i++)
	{
		rectangle(x[i],y[i],x[i]+10,y[i]+10);
			h++;
	    setcolor(h);
	}
		h=4;
	while(1)
	{
        nn=rand()%10+6;
        setcolor(nn);
        rectangle(48,48,512,412);
		if(flag==0)
		{
			srand((unsigned)time(NULL));
			food.x=rand()%400+50;
			food.y=rand()%300+50;
			while(food.x%10!=0)food.x++;
			while(food.y%10!=0)food.y++;
			flag=1;
			setcolor(YELLOW);
			rectangle(food.x,food.y,food.x+10,food.y+10);
		}
		if(food.x==x[0]&&food.y==y[0])
		{
			setcolor(BLACK);
			rectangle(food.x,food.y,food.x+10,food.y+10);
			n++;
			flag=0;
			score+=100;
			itoa(score,str,10);
			setcolor(m);
			outtextxy(450,30,str);
			m++;
		}
		Sleep(q);
		setcolor(BLACK);
		for(i=0;i<n;i++)
		{
				rectangle(x[i],y[i],x[i]+10,y[i]+10);
		}
		setcolor(YELLOW);
		rectangle(food.x,food.y,food.x+10,food.y+10);
		setcolor(5);
		rectangle(48,48,512,412);
		for(i=n-1;i>=1;i--)
		{
			x[i]=x[i-1];
			y[i]=y[i-1];
		}
		if(kbhit()!=0)
		{
			key=getch();
			switch(key)
			{
				case 119:if(d!=2)d=1;break;
				case 115:if(d!=1)d=2;break;
				case 97:if(d!=4)d=3;break;
				case 100:if(d!=3)d=4;break;
				case  27:d=5;break;
			}
		}
		switch(d)
		{	
			case 1:y[0]-=10;break;
			case 2:y[0]+=10;break;
			case 3:x[0]-=10;break;
			case 4:x[0]+=10;break;
		}
		for(i=3;i<n;i++)
		{
			if(x[i]==x[0]&&y[i]==y[0])
				stop=1;
		}
			if(stop==1)
				break;
		setcolor(3);
		for(i=0;i<n;i++)
		{
			rectangle(x[i],y[i],x[i]+10,y[i]+10);
			setcolor(h);
			h++;
		}
			h=4;
		if(x[0]<50&&x[0]>0||y[0]<50&&y[0]>0||x[0]>500&&x[0]<640||y[0]>400&&y[0]<480)
			{	
				kk+=1;
			}
			if(kk==3)
			{
			 	setcolor(BLACK);
				outtextxy(400,15,"还剩下一条命");
				setcolor(RED);
				outtextxy(400,15,"已无命留下");
				setcolor(BLACK); 
	            circle(280,30,5);
				break;
			}
			else
			if(kk==1)
			{
				setcolor(BLACK);
			    outtextxy(400,15,"还剩下两条命");
				setcolor(RED);
				outtextxy(400,15,"还剩下一条命");
				setcolor(BLACK);
				circle(250,30,5);
			}	
			else
			if(kk==2)
			{
				setcolor(BLACK);
				outtextxy(400,15,"还有三条命");
				setcolor(RED);
			    outtextxy(400,15,"还剩下两条命");
				setcolor(BLACK);
	            circle(265,30,5);	
			}
			else
			if(kk==0) 
			{
				setcolor(RED);
				outtextxy(400,15,"还有三条命");
			}
	}
		if(d!=5)
	  {
	    setcolor(YELLOW);
	    outtextxy(250,240,"GAME OVER!"); 
	    Sleep(3000);
	  } 
	closegraph();
	initwindow(640,480);
	setcolor(WHITE);
    rectangle(48,48,512,412);
	outtextxy(200,200,"您的得分为：");
	itoa(score,str,10);
	outtextxy(285,200,str);
	rectangle(305,330,370,350);
	outtextxy(310,333,"F:排行榜");
	ch8=getch();
	if(ch8=='F')
    {
    	xiao3=1;
    	setcolor(BLACK);
    rectangle(48,48,512,412);
	outtextxy(200,200,"您的得分为：");
	itoa(score,str,10);
	outtextxy(285,200,str);
	rectangle(320,330,370,350);
	outtextxy(325,333,"F:返回");
		phb();
	hh7=paihangbang();
				 if(hh7==1)
				 xiao3=1;
				 else
				 if(hh7==2)
				 xiao3=3; 
    }
    }
    else
    {
    setcolor(WHITE);
    outtextxy(200,220,"用户名或密码错误");
	rectangle(320,330,370,350);
	outtextxy(325,333,"F:返回");
	ch8=getch();
	if(ch8=='F')
    {
		xiao3=2;
    }
    }
    return xiao3;
}
int guizhe()//上下左右方向键控制贪吃蛇游动，不要撞到墙、不要碰到自己身体.
{
	setcolor(BLACK);
	circle(450,375,30);
	outtextxy(425,370,"E:规则"); 
	setcolor(BLUE);
	outtextxy(200,150,"w代表向上转弯，");
	setcolor(GREEN);
	outtextxy(200,175,"s代表向下转弯，");
	setcolor(RED); 
	outtextxy(200,200,"a代表向左转弯，");
	setcolor(YELLOW); 
	outtextxy(200,225,"d代表向右转弯，");
	setcolor(WHITE);
	outtextxy(145,250,"通过这四个键进行贪吃蛇的移动，"); 
	outtextxy(145,275,"每吃一个食物，分数就增加100分，"); 
	outtextxy(145,300,"一旦撞到墙壁或撞到自身就会死亡。");  
	circle(100,375,30);
	outtextxy(75,370,"F:返回"); 
	circle(450,375,30);
	outtextxy(425,370,"T:退出"); 
	ch7=getch();
	switch(ch7)
			{
				case 'F': xiao=1;break;
				case 'T': xiao=2;break;
				case '\0':getch();	
			}
	return xiao;
}
